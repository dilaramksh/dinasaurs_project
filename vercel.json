{
    "builds": [{
        "src": "dinasaurs/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "50mb", "runtime": "python3.9"}
    }],
    "env": {
        "PYTHON_VERSION": "3.9"
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dinasaurs/wsgi.py"
        }
    ]
}